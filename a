				(function($) {

					var espn = window.espn || {};
					espn.gamepackage = espn.gamepackage || {};
					espn.gamepackage.gameId = "400975286";
					espn.gamepackage.type = "playbyplay";
					espn.gamepackage.timestamp = "2018-01-01T00:00Z";
					espn.gamepackage.status = "post";
					espn.gamepackage.league = "nba";
					espn.gamepackage.sport = "basketball";
					espn.gamepackage.network = "";
					espn.gamepackage.awayTeamName = "los-angeles-lakers";
					espn.gamepackage.homeTeamName = "houston-rockets";
					espn.gamepackage.awayTeamId = "13";
					espn.gamepackage.homeTeamId = "10";
					espn.gamepackage.showGamebreak = true;
					
						espn.gamepackage.numPeriods = 4;
						espn.gamepackage.periodType = "Quarter";
						espn.gamepackage.minsInPeriod = "12";
					
					

					

					
						espn.gamepackage.probability = espn.gamepackage.probability || {};
						espn.gamepackage.probability.data = [];
					

					

						
						

						

						function loadScripts() {
							var src = "http://a.espncdn.com/redesign/0.390.12/js/dist/gamepackage-static.min.js",
								script;

							if (espn.gamepackage.status === "in") {
								if (!espn.gamepackage.core) {
									src = src.replace("gamepackage.", "gamepackage.core.");
								} else if (espn.gamepackage.bundles && typeof espn.gamepackage.bundles[espn.gamepackage.sport] === "function") {
									espn.gamepackage.init();
									espn.gamepackage.bundles[espn.gamepackage.sport]();
									return;
								}
							} else if (espn.gamepackage.init) {
								espn.gamepackage.init();
								return;
							}

							script = document.createElement("script");
							script.type = "text/javascript";
							script.src = src;
							document.getElementsByTagName("head")[0].appendChild(script);
						}

						
							if (window.espn_ui.deferReady === true) {
								$(document).one("page.render", loadScripts);
							} else {
								$.subscribe("espn.defer.end", loadScripts);
							}
						

						if( typeof(newrelic) !== "undefined" ){
							newrelic.setCustomAttribute( "game-state", espn.gamepackage.status );
						}
					
				})(jQuery);
